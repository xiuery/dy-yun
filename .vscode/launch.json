{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "go-admin (dev)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}/../go-admin",
      "args": [
        "server",
        "-c",
        "config/settings.sqlite.yml",
        "-a"
      ],
      "buildFlags": "-tags=sqlite3",
      "console": "integratedTerminal"
    },
    {
      "name": "go-admin-ui",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/../go-admin-ui",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Python: dy-yun (开发环境)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": ["-c", "config/settings.dev.yaml"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": false,
      "django": false
    },
    {
      "name": "Python: dy-yun (默认配置)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": false
    },
    {
      "name": "Python: 当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": true
    }
  ]
}
